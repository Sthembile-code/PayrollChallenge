//UnitTestClass
using System;
using Microsoft.VisualStudio.TestTools.UnitTesting;


namespace PayrollChallenge.Tests
{
    [TestClass]
    public class UnitTest1
    {
        [TestMethod]
        public void TestCalculateSalary()
        {
            Employee emp = new Employee();
            double results = emp.CalculateSalary(10, 20);
            Assert.AreEqual(200, results);
        }
    }
}

//EmployeeClass

using System;
using System.Collections.Generic;
using System.Linq;
using System.Web;

namespace PayrollChallenge
{
    public class Employee
    {
        public double CalculateSalary(double hours, double rate)
        {
            return (hours * rate);
        }
    }
}